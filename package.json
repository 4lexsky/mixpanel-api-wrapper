{
  "name": "mixpanel-jql",
  "description": "wrapper around the mixpanel-data-export module to create an easy interface for retrieiving the results of JQL queries in Mixpanel",
  "version": "0.0.0",
  "scripts": {
    "fmt": "mongodb-js-fmt",
    "check": "mongodb-js-precommit",
