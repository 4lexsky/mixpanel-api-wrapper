    "sinon": "^1.17.4",
    "yargs": "^4.7.1"
  },
  "devDependencies": {
    "eslint-config-mongodb-js": "^1.0.6",
    "mocha": "^2.3.4",
    "mongodb-js-fmt": "^0.0.3",
    "mongodb-js-precommit": "^0.2.8",
    "pre-commit": "^1.1.2"
  },
  "license": "Apache-2.0",
  "precommit": [
    "check"
